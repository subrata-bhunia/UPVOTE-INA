{
  "name": "Upvote",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "clean": "cd android && ./gradlew clean && cd ../",
    "load": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "clear": "cd android && ./gradlew cleanBuildCache && cd ../",
    "reset": "react-native start --reset-cache",
    "update:pods": "cd ios && pod install --repo-update && cd ..",
    "apk": "cd android && ./gradlew assembleRelease && cd ../",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-navigation/drawer": "^6.1.4",
    "@react-navigation/material-top-tabs": "^6.0.2",
